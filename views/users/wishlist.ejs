						<h4>Your wishlist contains: <span></span></h4>
					<table class="timetable_sub">
						<thead>
							<tr>
								<th>SL No.</th>	
								<th>Product</th>
								<!-- <th>Quantity</th> -->
								<th>Product Name</th>
							
								<th>Price</th>
								<th>Move to Cart</th>
								<th>Remove</th>
							
							</tr>
						</thead>
						<tbody>
							<% user.forEach(function(user,index) { %>
						<tr class="rem1">
							<td class="invert"><%=index+1 %>%></td>
