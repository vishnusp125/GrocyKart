<script src="https://cdn.plot.ly/plotly-2.16.1.min.js"></script>
<script>
  var getTest = [<%=sales%>];
  var saletime = ['<%=timeOfSale%>']
  console.log(saletime);
  console.log(getTest);
  
  var xArray = saletime[0].split(',')
  console.log(xArray);
  var barData = [{
    x: xArray,
    y: getTest,
    mode: "lines",
    type: "bar"
  }];
  let data = [{
    x: xArray,
    y: getTest,
    mode: "markers",
    type: "scatter"
  }];

  var barLayout = {
    xaxis: {
      labels: [xArray],
      title: 'Date',

    },
    yaxis: {
      labels: [getTest],
      title: "Sales"
    },
    title: "Sales Report"
  };

  let layout = {
    xaxis: {
      type: 'date'
    },
    yaxis: {
      title: "Sales"
    },
    title: "Individual orders"
  };
  console.log(xArray)
  //  console.log(getTest);
  //  console.log(saletime);
  TESTER = document.getElementById('tester');
  TESTER2 = document.getElementById('tester2');
  Plotly.newPlot(TESTER, barData, barLayout)
  Plotly.newPlot(TESTER2, data, layout)

</script>


